{
  "multipart": [
    {
      "apply": {
        "model": "spelunkers_charm:block/web_vein_north"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "spelunkers_charm:block/web_vein_east",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "spelunkers_charm:block/web_vein_south",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "spelunkers_charm:block/web_vein_west",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "spelunkers_charm:block/web_vein_up",
        "uvlock": true,
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "spelunkers_charm:block/web_vein_down",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "down": "true"
      }
    }
  ]
}